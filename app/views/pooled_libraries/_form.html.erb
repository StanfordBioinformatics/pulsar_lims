<%= simple_form_for(@pooled_library) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :paired_end %>
  <%= f.association :sequencing_library_prep_kit %>
  <%= f.input :size_range %>

  <div class="form-actions">
    <%= f.button :submit, class: "btn-primary" %>
  </div></br>
<% end %>
