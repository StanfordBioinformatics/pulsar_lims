<%= simple_form_for(@crispr) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :user %>
    <%= f.association :crispr_construct %>
    <%= f.association :donor_construct %>
    <%= f.association :biosample %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
