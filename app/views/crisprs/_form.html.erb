<%= simple_form_for(@crispr) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.association :crispr_construct %>
  <%= f.association :donor_construct %>
  <%= f.association :biosample %>

  <div class="form-actions">
    <%= f.button :submit, class: "btn-primary"  %>
  </div></br>
<% end %>
