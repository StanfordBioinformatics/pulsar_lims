<%= simple_form_for @agarose_gel, url: {action: "create_gel_lane"}, html: {remote: true, method: :post, class: "gel-lane-form"} do |f| %>
  <%= f.simple_fields_for :gel_lanes, @gel_lane do |ff| %>
    <%= render partial: "gel_lanes/form", locals: {f: ff} %>
  <% end %>
  <%= f.button :submit, "Create", action: :create_gel_lane, class: "btn-primary agarose-gel-create-lane-btn" %>
<% end %>
