<%= simple_form_for(@experiment_type) do |f| %>
  <%= render partial:  "application_partials/show_form_error_messages", locals: {:model_obj => @experiment_type } %>

  <%= f.input :name, label_html: {"data-toggle" => "tooltip", "title" => "Unique experiment type name"} %>

  <div class="actions">
    <%= f.button :submit, class: "btn-primary" %>
  </div>
<% end %>
