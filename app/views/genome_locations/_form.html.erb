<%= simple_form_for(@genome_location) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :user %>
    <%= f.association :chromosome %>
    <%= f.input :start %>
    <%= f.input :end %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
