<p id="notice"><%= notice %></p>

<h1>Listing Library Sequencing Results</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sequencing result</th>
      <th>Library</th>
      <th>Comment</th>
      <th>Read1 uri</th>
      <th>Read2 uri</th>
      <th>Read1 count</th>
      <th>Read2 count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @library_sequencing_results.each do |res| %>
      <tr>
        <td><%= res.name %></td>
        <td><%= res.sequencing_result %></td>
        <td><%= res.library %></td>
        <td><%= res.comment %></td>
        <td><%= res.read1_uri %></td>
        <td><%= res.read2_uri %></td>
        <td><%= res.read1_count %></td>
        <td><%= res.read2_count %></td>
        <td><%= link_to 'Show', [@sequencing_request,@sequencing_result,res] %></td>
        <td><%= link_to 'Edit', [:edit,@sequencing_request,@sequencing_result,res] %></td>
        <td><%= link_to 'Destroy', [@sequencing_request,@sequencing_result,res], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Library sequencing result', new_library_sequencing_result_path %>
