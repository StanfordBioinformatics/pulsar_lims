<header>
	<h1>New Plate</h1>
</header>
<%= simple_form_for(@plate) do |f| %>
	<%= render partial:  "application_partials/show_form_error_messages", locals: {:model_obj => @plate } %>
	<%= render partial: 'form', locals: {f: f} %>
	<%= render partial: "submit", locals: {f: f} %>
<% end %>

<%= link_to 'Back', plates_path %>
