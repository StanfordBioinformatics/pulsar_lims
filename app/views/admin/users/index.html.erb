<header>
	<h1>Users</h1>
	
	<ul class="actions">
		<li>
			<%= link_to "New User", new_admin_user_path, class: "new" %>
		</li>
	</ul>
</header>

	
<div id="admins">	
	<h4>Admin Users</h4>
	<ul>
		<% @admins.each do |admin| %>
			<li><%= link_to admin, [:admin, admin] %></li>
		<% end %>
	</ul>
</div>

</br>

<div id="users">	
	<h4>Regular Users</h4>
	<ul>
		<% @users.each do |user| %>
			<li><%= link_to user, [:admin, user] %></li>
		<% end %>
	</ul>
</div>
