<header>
  <h1><%= @uberon.name %></h1>

  <ul class="actions">
    <%= render partial: "application_partials/auth_edit_record", locals: {record: @uberon} %>
    <%= render partial: "application_partials/auth_destroy_record", locals: {record: @uberon} %>
  </ul>
</header>

<%= render partial: "application_partials/record_meta", locals: { record: @uberon } %>

<table class="attributes">
	<tr>
		<th><strong>Name:</strong></th>
		<td><%= @uberon.name %></td>
	</tr>

	<tr>
		<th><strong>Accession:</strong></th>
		<td><%= @uberon.accession %></td>
	</tr>
</table>

<%= link_to 'Back', uberons_path %>
