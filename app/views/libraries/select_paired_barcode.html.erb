<%= simple_fields_for @library do |f| %>
	<%= f.association :paired_barcodes, label: "Add paired barcodes",prompt: "Select paired barcodes", collection: @paired_barcodes.map {|x| [x.display,x.id]}, label_html: {multiple: true, "data-toggle" => "tooltip", "title" => "If the library has paired-end barcode(s), select the paired-end barcode(s) used."} %>
<% end %>
