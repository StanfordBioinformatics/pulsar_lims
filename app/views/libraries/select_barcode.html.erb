<%= simple_fields_for @library do |f| %>
	<%= f.association :barcodes, collection: @index1_barcodes.map {|x| ["#{x.name}  #{x.sequence}",x.id]}, label_html: {multiple: true, "data-toggle" => "tooltip", "title" => "If the library is barcoded, select the barcode used. If the library is pooled having multiple barcodes, select all barcodes used in the pooling."} %>
<% end %>
