<% if defined?(exclude_library_ids) %>
  <%= f.association :libraries, collection: Library.non_plated.where.not(id: exclude_library_ids).order(id: :desc), prompt: "Select a Library", input_html: {multiple: true} %>
<% else %>
  <%= f.association :libraries, collection: Library.non_plated.order(id: :desc), prompt: "Select a Library", input_html: {multiple: true} %>
<% end %>
  
