<% if defined?(exclude_library_ids) %>
	<%= f.association :libraries, collection: Library.where.not(id: [exclude_library_ids]), prompt: "Select a Library", input_html: {multiple: true} %>
<% else %>
	<%= f.association :libraries, collection: Library.order(:name), prompt: "Select a Library", input_html: {multiple: true} %>
<% end %>
	
