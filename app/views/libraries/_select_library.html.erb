<% if defined?(exclude_library_ids) %>
	<%= f.association :libraries, collection: Library.non_plated_non_prototype.where.not(id: exclude_library_ids), prompt: "Select a Library", input_html: {multiple: true} %>
<% else %>
	<%= f.association :libraries, collection: Library.non_plated_non_prototype.order(:name), prompt: "Select a Library", input_html: {multiple: true} %>
<% end %>
	
