<header>
	<h1>Antibody Purifications</h1>

  <ul class="actions">
		<%= render partial: "application_partials/auth_new_record", locals: {model_class: AntibodyPurification} %>
  </ul> 
</header>

<table class="attributes">
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @antibody_purifications.each do |antibody_purification| %>
      <tr>
        <td><%= link_to antibody_purification.name, antibody_purification %></td>
        <td>
					<%= link_to fa_icon("pencil"), edit_antibody_purification_path(antibody_purification) %>
        	<%= link_to fa_icon("trash"), antibody_purification, method: :delete, data: { confirm: 'Are you sure?' } %>
				</td>
      </tr>
    <% end %>
  </tbody>
</table>
