<%= simple_form_for(@library_fragmentation_method) do |f| %>
  <%= render partial:  "application_partials/show_form_error_messages", locals: {:model_obj => @library_fragmentation_method } %>

  <%= f.input :name, label_html: {"data-toggle" => "tooltip","title" => "Unique library name"} %>
  <%= f.input :description, label_html: {"data-toggle" => "tooltip", "title" => "The description of fragmentation method." } %>
  <%= render partial: "application_partials/notes", locals: {f: f} %>

  <div class="form-actions">
    <%= f.button :submit, class: "btn-primary" %>
  </div></br>
<% end %>
