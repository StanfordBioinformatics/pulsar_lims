<%= simple_form_for(@reference_genome) do |f| %>
	<%= render partial:  "application_partials/show_form_error_messages", locals: {:model_obj => @reference_genome } %>

  <%= f.input :name, label_html: {"data-toggle" => "tooltip", "title" => "Unique reference genome name"} %>
  <%= f.input :url, label_html: {"data-toggle" => "tooltip", "title" => "External link providing details on the reference genome build."} %>
  <%= render partial: "application_partials/notes", locals: {f: f} %>

  <div class="actions">
    <%= f.button :submit, class: "btn-primary"%>
  </div></br>
<% end %>
