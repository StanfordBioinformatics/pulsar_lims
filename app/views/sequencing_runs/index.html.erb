<header>
  <h1 class="index-page-header">Sequencing Results</h1>
	<p class="model-definition"><%= SequencingRun::DEFINITION %></p>

  <ul class="actions">
    <%= render partial: "application_partials/auth_new_record", locals: {model_class: SequencingRun} %>
  </ul>   
</header>

<table class="attributes">
  <thead>
    <tr>
      <%= render partial: "application_partials/index_common_table_headers" %>
      <th>Comment</th>
      <th>User</th>
      <th>Sequencing request</th>
      <th>Lane</th>
      <th>Comment</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @sequencing_runs.each do |rec| %>
      <tr>
        <%= render partial: "application_partials/index_common_table_cells", locals: {record: rec} %>
        <td><%= rec.comment %></td>
        <td><%= link_to rec.sequencing_request.name, rec.sequencing_request %></td>
        <td><%= rec.lane %></td>
				<td>
					<%= link_to fa_icon("pencil"), edit_sequencing_run(library) %>
        	<%= link_to 'Destroy', rec, method: :delete, data: { confirm: 'Are you sure?' } %>
				</td>
      </tr>
    <% end %>
  </tbody>
</table>
