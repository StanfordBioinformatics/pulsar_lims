<%= simple_form_for @single_cell_sorting do |f| %>
	<%= f.simple_fields_for :plates, @plate  do |ff| %>
		<%= render partial: "plates/form", locals: {f: ff} %>
	<% end %>
  <div class="form-actions">
    <%= f.button :submit, "Create Plate", class: "btn-primary" %>
  </div></br>
	<button class="toggle-up" type="button" class="btn btn-link"><%= fa_icon("toggle-up lg") %></button>
<% end %>
