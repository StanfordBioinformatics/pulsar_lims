<p id="notice"><%= notice %></p>

<h1>Listing Sequencing Centers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sequencing_centers.each do |sequencing_center| %>
      <tr>
        <td><%= sequencing_center.name %></td>
        <td><%= sequencing_center.address %></td>
        <td><%= link_to 'Show', sequencing_center %></td>
        <td><%= link_to 'Edit', edit_sequencing_center_path(sequencing_center) %></td>
        <td><%= link_to 'Destroy', sequencing_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sequencing center', new_sequencing_center_path %>
